{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-underscore-dangle": "off",
        "prettier/prettier": ["error", {"singleQuote": true, "printWidth": 100, "tabWidth": 4, "bracketSpacing": false }]
    }
}